<h1>Inscrições</h1>

<table>
  <tr>
    <th>Processo seletivo</th>
    <th>Data final</th>
    <th>Descrição</th>
    <th></th>
  </tr>
<% @selections = Selection.all %>
<% @selections.each do |s| %>
  <tr>
    <td><%= s.title %></td>
    <td><%= s.entries_end_date %></td>
    <td><%= s.description %></td>
    <td><%= link_to 'Inscrever-se', new_candidate_path %></td>
  </tr>
<% end %>
</table>

<br />
